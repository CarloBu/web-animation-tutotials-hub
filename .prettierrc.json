{
  "plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.astro, *.mjs",
      "options": {
        "printWidth": 100,
        "useTabs": true,
        "singleQuote": true,
        "trailingComma": "all",
        "semi": true,
        "parser": "astro"
      }
    }
  ]
}
